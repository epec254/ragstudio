{
  "num_workers": {
    "type": "unlimited",
    "defaultValue": 13
  },
  "node_type_id": {
    "type": "unlimited",
    "defaultValue": "c5.9xlarge"
  },
  "spark_env_vars.refGenomeId": {
    "type": "unlimited",
    "defaultValue": "grch38"
  },
  "spark_version": {
    "type": "regex",
    "pattern": ".*-hls.*",
    "defaultValue": "7.4.x-hls-scala2.12"
  },
  "aws_attributes.ebs_volume_count": {
    "type": "unlimited",
    "defaultValue": 3
  },
  "aws_attributes.ebs_volume_size": {
    "type": "unlimited",
    "defaultValue": 200
  }
}
